{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}